
{
  "name": "firebase-storage-rule-test-example",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "yarn firebase emulators:exec --only storage 'jest --forceExit'",
    "test:watch": "yarn firebase emulators:exec --only storage 'jest --watch'"
  },
  "dependencies": {
    "firebase": "^9.1.2"
  },
  "devDependencies": {
    "firebase-tools": "^9.19.0",
    "@firebase/rules-unit-testing": "^2.0.1",
    "@types/jest": "^27.0.2",
    "jest": "^27.2.4",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testMatch": [
      "**/*.test.ts"
    ]
  }
}